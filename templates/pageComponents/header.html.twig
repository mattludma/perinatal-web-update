<header class="header {{ _headerClass|default() }}" role="banner">
  {% set locale = app.request.locale %}
  {% if getComponent('commonBannerTop') %}
    {% set banner = getComponent('commonBannerTop').data %}
    {% if banner['displayDesktopBanner'] or banner['displayMobileBanner'] %}
      <div class="toolbar show-desktop">
        <div class="container">
          <p><span id="banner-desktop" href="{{ banner['slides']|first['bannerLink'][locale] }}" data-values="{% for slide in banner['slides'] %}{{ slide['desktopBanner'][locale] }}{% if not loop.last %};{% endif %}{% endfor %}" data-time="{{ banner['time'] }}" data-href="{% for slide in banner['slides'] %}{{ slide['bannerLink'][locale] }}{% if not loop.last %};{% endif %}{% endfor %}">{{ banner['slides']|first['desktopBanner'][locale] }}</span></p>
        </div>
      </div>
    {% endif %}
  {% endif %}

